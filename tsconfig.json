{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "rootDir": "./src",         // Diretório de entrada dos arquivos TypeScript
    "outDir": "./dist",         // Diretório de saída dos arquivos compilados
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
